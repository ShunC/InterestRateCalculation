        <servlet-name>InterestRateServlet</servlet-name>
        <servlet-class>Servlet.InterestRateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InterestRateServlet</servlet-name>
        <url-pattern>/InterestRateServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>YearServlet</servlet-name>
