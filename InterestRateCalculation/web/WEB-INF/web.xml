    </servlet>
    <servlet-mapping>
        <servlet-name>YearServlet</servlet-name>
        <url-pattern>/YearServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PrincipalServlet</servlet-name>
        <servlet-class>Servlet.PrincipalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrincipalServlet</servlet-name>
        <url-pattern>/PrincipalServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InterestServlet</servlet-name>
        <servlet-class>Servlet.InterestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InterestServlet</servlet-name>
        <url-pattern>/InterestServlet</url-pattern>
    </servlet-mapping>

